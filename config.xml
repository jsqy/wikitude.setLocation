<?xml version='1.0' encoding='utf-8'?>
<widget id="com.wikitude.setLocation" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>setLocation</name>
    <description>
        a sample cordova wikitude application to demonstrate wikitude.setLocation method
    </description>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
</widget>
